SELECT "estatal_delitos"."Year",
	ESTATAL_DELITOS."Bien_juridico_afectado",
	SUM(ESTATAL_DELITOS."Enero") AS ENERO,
	SUM(ESTATAL_DELITOS."Febrero") AS FEBRERO,
	SUM(ESTATAL_DELITOS."Marzo") AS MARZO,
	SUM(ESTATAL_DELITOS."Abril") AS ABRIL,
	SUM(ESTATAL_DELITOS."Mayo") AS MAYO,
	SUM(ESTATAL_DELITOS."Junio") AS JUNIO,
	SUM(ESTATAL_DELITOS."Julio") AS JULIO,
	SUM(ESTATAL_DELITOS."Agosto") AS AGOSTO,
	SUM(ESTATAL_DELITOS."Septiembre") AS SEPTIEMBRE,
	SUM(ESTATAL_DELITOS."Octubre") AS OCTUBRE,
	SUM(ESTATAL_DELITOS."Noviembre") AS NOVIEMBRE,
	SUM(ESTATAL_DELITOS."Diciembre") AS DICIEMBRE,
	"00ent"."geom"
FROM ESTATAL_DELITOS
JOIN "00ent" ON ESTATAL_DELITOS."Clave_Ent" = "00ent"."gid"
GROUP BY ESTATAL_DELITOS."Year",
	"00ent"."gid",
	ESTATAL_DELITOS."Bien_juridico_afectado"
ORDER BY ESTATAL_DELITOS."Year",
	"00ent"."gid",
	ESTATAL_DELITOS."Bien_juridico_afectado"